{
    "version": 2,
    "builds": [
      {
        "src": "seagull/wsgi.py",
        "use": "@vercel/python"
      },
      {
        "src": "build_files.sh",
        "use": "@vercel/static-build",
        "config": {
          "distDir": "seagull/staticfiles_build"
        }
      }
    ],
    "routes": [
      {
        "src": "/static/(.*)",
        "dest": "/seagull/staticfiles_build/$1"
      },
      {
        "src": "/(.*)",
        "dest": "seagull/wsgi.py"
      }
    ]
}